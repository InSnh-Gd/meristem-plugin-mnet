{
  "id": "com.meristem.mnet",
  "name": "M-Net Overlay Network",
  "version": "1.0.0",
  "tier": "core",
  "runtime_profile": "sandbox",
  "sdui_version": "1.0",
  "dependencies": [],
  "entry": "src/index.ts",
  "ui": {
    "mode": "SDUI",
    "entry": "ui/index.html"
  },
  "ui_contract": {
    "route": "/mnet",
    "channels": [
      "sys.network.mode"
    ],
    "default_log_level": "info",
    "stream_profile": "balanced"
  },
  "permissions": [
    "sys:manage",
    "node:read",
    "node:cmd"
  ],
  "events": [
    "node.join",
    "node.offline"
  ],
  "exports": [
    "network-mode-status",
    "network-authkey",
    "network-derp-map"
  ]
}
